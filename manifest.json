{
    "name": "UERJ Extension",
    "version": "1.0",
    "description":"Improve student life using AVA",
    "manifest_version": 2,
    "icons":{
        "16":"/images/icon16.png",
        "48":"/images/icon48.png",
        "128":"/images/icon128.png"
    },
    "page_action":{
        "default_icon":{
            "16":"/images/icon16.png",
            "48":"/images/icon48.png",
            "128":"/images/icon128.png"
        }
    },
    "content_scripts":[
        {
            "matches": ["https://ava.pr1.uerj.br/*"],
            "css":["main.css"],
            "js":["main.js"]
        }
    ]
  }